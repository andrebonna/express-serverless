{
  "name": "api",
  "version": "1.0.0",
  "description": "AWSApi",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --quiet src/**/*.js src/*.js",
    "test": "jest",
    "start": "node server.js"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "html"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "js-yaml": "^3.9.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^17.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "jest": "^17.0.0",
    "jest-cli": "^17.0.0",
    "jest-junit-reporter": "^1.1.0",
    "pre-commit": "^1.2.2"
  }
}
