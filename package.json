{
  "name": "serverless-universal-react",
  "version": "1.0.0",
  "description": "ServerlessUniversalReact",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --quiet src/**/*.js src/*.js",
    "test": "jest",
    "transpile": "gulp",
    "bundle": "webpack",
    "start": "npm run transpile && npm run bundle && node build/server.js",
    "deploy": "npm run transpile && serverless deploy"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "html"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "js-yaml": "^3.9.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^17.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.3.2",
    "jest": "^17.0.0",
    "jest-cli": "^17.0.0",
    "jest-junit-reporter": "^1.1.0",
    "node-sass": "^4.5.3",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6"
  }
}
