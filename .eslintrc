{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "json"
    ],
    "rules": {
        "comma-dangle": [
            2,
            "never"
        ],
        "valid-jsdoc": 0,
        "require-jsdoc": 0,
        "padded-blocks": 0,
        "no-undef": 0,
        "max-len": 0,
        "jsx-a11y/href-no-hash": 0,
        "import/no-dynamic-require": 0,
        "import/newline-after-import": 0,
        "semi": 2
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
